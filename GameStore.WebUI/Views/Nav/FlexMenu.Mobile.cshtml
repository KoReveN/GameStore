@model IEnumerable<string>


<div data-role="navbar">
    <ul>
        @foreach (var link in Model)
        {
        <li>
            @Html.RouteLink(
            link,  // LinkText
            new {   //RouteValues
                controller = "Game",
                action = "List",
                category = link,
                page = 1
            },
            new{ // HtmlAttributes
                data_transition = "fade",
                @class = (link == ViewBag.SelectedCategory ? " ui-btn-active" : null)
            }
            )
        </li>
        }
    </ul>






</div>
